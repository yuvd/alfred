<ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#list" role="tab" aria-controls="home" aria-selected="true">List</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#schedule" role="tab" aria-controls="profile" aria-selected="false">Schedule</a>
  </li>
</ul>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="list" role="tabpanel" aria-labelledby="home-tab">
    <% @bookmarks.each do |bookmark| %>
    <h1><%= bookmark.place.name%></h1>
    <h5><%= bookmark.time %></h2>
<%end%>
  </div>
  <div class="tab-pane fade" id="schedule" role="tabpanel" aria-labelledby="profile-tab">
    <% @bookmarks.each do |bookmark| %>
    <%unless bookmark.time.nil?%>
      <h1><%= bookmark.place.name%></h1>
      <h5><%= bookmark.time %></h2>
    <%end%>
<%end%>
  </div>
</div>

